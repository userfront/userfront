{
  "name": "@userfront/node",
  "version": "1.0.1",
  "description": "TypeScript library for Userfront Node.js SDK",
  "repository": {
    "type": "git",
    "url": "https://github.com/userfront/userfront.git"
  },
  "keywords": [
    "userfront",
    "authentication",
    "authorization",
    "javascript",
    "typescript",
    "node",
    "sdk",
    "express"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "test:watch": "vitest watch",
    "test": "vitest run",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@userfront/tsconfig": "workspace:*",
    "eslint": "9.2.0",
    "msw": "2.3.0",
    "tsup": "8.0.2",
    "type-fest": "4.18.2",
    "typescript": "5.4.5",
    "vitest": "1.6.0"
  },
  "engines": {
    "node": ">=20"
  }
}
